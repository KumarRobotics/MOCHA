  <include file="$(find topic_publisher)/launch/visualize.launch" />
  <node pkg="rosbag" type="record" name="rosbag_record_basestation"
	  args="record -o /home/dcist/data/$(arg bagname) /iapetus/asoom/map_sem_img /iapetus/asoom/map_sem_img_center_hash /iapetus/asoom/recent_key_pose_hash /iapetus/asoom/map_sem_img_center_hash /iapetus/asoom/recent_key_pose_hash /rosout_agg" />
</launch>

