<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="robot_ip"/>
  <include file="$(find distributed_database)/launch/integrate_commsdb.launch">
    <arg name="client_ip_ups" value="$(arg robot_ip)" />
  </include>
  <node name = "input_topic_throttler" type="drop" pkg="topic_tools" args= "/upslam/global_pose 9 10 /upslam/global_pose_throttled" />
  <node pkg="translators" type="jackal_translator.py" name="jackal_translator" output="screen"/>
  <node pkg="topic_publisher" type="publish_jackal.py" name="publish_jackal" output="screen"/>
</launch>
